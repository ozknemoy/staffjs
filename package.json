{
  "name": "angular-nestjs-render",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run server:watch\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "client:build": "ng build --prod",
    "client:test": "ng test",
    "client:lint": "ng lint --fix",
    "server:lint": "tslint  --project ./tsconfig.json './src/server/**/*.ts' -c tslint.server.json --fix",
    "server:test": "jasmine-ts ./src/server/**/*.spec.ts",
    "e2e": "ng e2e",
    "server:watch": "nodemon",
    "build:universal": "npm run helper:build:client-and-server && npm run helper:webpack:nest",
    "serve:universal": "node dist/server.js",
    "helper:build:client-and-server": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "helper:webpack:nest": "webpack --config webpack.server.config.js --colors",
    "prettify": "pretty-quick"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/microservices": "^5.0.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/testing": "^5.0.0",
    "@nestjs/websockets": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "bootstrap": "^4.1.3",
    "copy-webpack-plugin": "^4.4.1",
    "core-js": "^2.4.1",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "docx": "^4.3.0",
    "dotenv": "^4.0.0",
    "file-saver": "^1.3.8",
    "multer": "^1.3.1",
    "ng2-file-upload": "^1.3.0",
    "ngx-bootstrap": "^3.1.1",
    "ngx-toastr": "^9.1.1",
    "node-xlsx": "^0.12.1",
    "officegen": "^0.4.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdfmake": "^0.1.37",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6",
    "webpack-node-externals": "^1.6.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.100",
    "codelyzer": "^4.0.0",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jasmine-ts": "^0.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.12",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "ts-loader": "^3.5.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "^2.9.2"
  }
}
